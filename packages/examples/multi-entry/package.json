{
  "name": "@parcel/multi-entry",
  "version": "2.0.0-beta.1",
  "license": "MIT",
  "private": true,
  "source": "src/index.js",
  "scripts": {
    "clean": "rm -rf ../.parcel-cache dist",
    "demo": "yarn clean && PARCEL_WORKERS=0 parcel build .",
    "debug": "yarn clean && PARCEL_WORKERS=0 node --inspect-brk ../../core/parcel/src/bin.js build .",
    "watch": "nodemon -w ../../ -i dist -i ../.parcel-cache -d 2 -x yarn demo"
  },
  "devDependencies": {
    "@parcel/babel-register": "2.0.0-beta.1",
    "@parcel/core": "2.0.0-beta.1"
  },
  "browser": "dist/browser/index.js",
  "alternate": "dist/alternate/index.js",
  "targets": {
    "alternate": {
      "source": "src/indexAlternate.js"
    },
    "browser": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    }
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
